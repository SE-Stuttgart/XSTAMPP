<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="xstpa.ui.View"
            id="xstpa.view.contextTables"
            name="%view.name.contextTables"
            restorable="true">
      </view>
   </extension>
   <extension
         point="xstampp.extension.stepEditors">
      <stepEditor
            editorId="xstpa.editor.context"
            icon="icons/logo.png"
            id="xstpa.stepEditor1"
            name="%stepEditor.name.contextTables"
            parentStep="astpa.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpa.editor.ruca"
            icon="icons/ruca_logo2x16.png"
            id="xstpa.views.ruca"
            name="%stepEditor.name.RefinedUnsafeControlActions"
            parentStep="astpa.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpa.editor.refinedSC"
            icon="icons/rsc_logox16.png"
            id="xstpa.stepEditor.RefinedSC"
            name="%stepEditor.name.RefinedSafetyConstraints"
            parentStep="astpa.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpa.editor.ltl"
            icon="icons/ltl.png"
            id="xstpa.stepEditor.ltl"
            name="%stepEditor.name.LTLFormulaTable"
            parentStep="astpa.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpa.editor.basicScenarios"
            id="xstpa.stepEditor.basicScenarios"
            name="Basic Scenarios"
            parentStep="astpa.navigation.step3_1">
      </stepEditor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="xstpa.settings.PreferencePageSettings"
            id="xstpa.preferences.choosePath"
            name="%page.name.XSTPA_Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
      <toc
            file="tocconcepts.xml">
      </toc>
      <toc
            file="tocgettingstarted.xml">
      </toc>
      <toc
            file="tocreference.xml">
      </toc>
      <toc
            file="tocsamples.xml">
      </toc>
      <toc
            file="toctasks.xml">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="xstpa.ui.RefinedUCAView"
            default="false"
            icon="icons/ruca_logo2x16.png"
            id="xstpa.editor.ruca"
            name="%editor.name.Refined_Unsafe_Control_Actions">
      </editor>
      <editor
            class="xstpa.ui.RefinedSafetyConstraintsView"
            default="false"
            icon="icons/rsc_logox16.png"
            id="xstpa.editor.refinedSC"
            name="%editor.name.Refined_Safety_Constraints">
      </editor>
      <editor
            class="xstpa.ui.tables.LTLPropertiesTable"
            default="false"
            icon="icons/ltl.png"
            id="xstpa.editor.ltl"
            name="%editor.name.LTL_Formulas">
      </editor>
      <editor
            class="xstpa.ui.CSContextEditor"
            default="false"
            id="xstpa.editor.context"
            name="%editor.name.contextTables">
      </editor>
      <editor
            class="xstpa.ui.BasicScenariosTable"
            default="false"
            id="xstpa.editor.basicScenarios"
            name="Basic Scenarios">
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="xstpa.settings.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
