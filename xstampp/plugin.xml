<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/xstampp/logo_small.png">
         <run
               class="xstampp.Application">
         </run>
      </application>
   </extension>
   <extension
         id="xstampp_product"
         point="org.eclipse.core.runtime.products">
      <product
            application="xstampp.application"
            name="%product.name">
         <property
               name="appName"
               value="STAMP Platform">
         </property>
         <property
               name="startupProgressRect"
               value="2,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
        </property>
  <property
        name="windowImages"
        value="icons/xstampp/logox16.png,icons/xstampp/logox32.png,icons/xstampp/logox48.png,icons/xstampp/logox64.png,icons/xstampp/logox128.png">
  </property>
  <property
        name="aboutImage"
        value="icons/xstampp/logox128.png">
  </property>
  <property
        name="aboutText"
        value="XSTAMPP - An eXtensible STAMP Platform for Safety Engineering&#x0A;&#x0A;Â©2014 - 2016&#x0A;http://www.iste.uni-stuttgart.de/se/werkzeuge/xstampp.html&#x0A;&#x0A;Lukas Balzer, Asim Abdulkhaleq, Stefan Wagner&#x0A;Institute of Software Technology, Software Engineering Group&#x0A;University of Stuttgart, Germany&#x0A;&#x0A;X-STPA &#x0A;http://www.iste.uni-stuttgart.de/se/werkzeuge/xstpa.html&#x0A;&#x0A;A-CAST&#x0A;http://www.iste.uni-stuttgart.de/se/werkzeuge/a-cast.html">
  </property>
      	</product>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="xstampp.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="xstampp.Perspective"
            fixed="true"
            id="astpa.welcome.perspective"
            name="%perspective.name.welcome">
      </perspective>
      <perspective
            class="xstampp.DefaultPerspective"
            fixed="false"
            icon="icons/xstampp/logox32.png"
            id="xstampp.defaultPerspective"
            name="%perspective.name.standard">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="xstampp.ui.welcome.WelcomeView"
            id="astpa.Welcome"
            name="%view.name.welcome"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            class="xstampp.ui.navigation.ProjectExplorer"
            fastViewWidthRatio="0.2f"
            id="astpa.explorer"
            name="%view.name.explorer"
            restorable="true">
      </view>
   </extension>
   <extension
         name="window"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="%menu.name.file">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  label="%command.name.newProject"
                  style="push"
                  tooltip="%command.tooltip.newProject">
               <visibleWhen
                     checkEnabled="true">
               </visibleWhen>
            </command>
            <command
                  commandId="xstampp.command.load"
                  label="%command.name.loadProject"
                  style="push"
                  tooltip="%command.tooltip.loadProject">
            </command>
            <separator
                  name="Separator 1"
                  visible="true">
            </separator>
            <command
                  commandId="astpa.save"
                  label="%command.name.save"
                  style="push">
            </command>
            <command
                  commandId="xstampp.saveall"
                  label="%command.name.saveall"
                  style="push">
            </command>
            <command
                  commandId="astpa.save"
                  label="%command.name.saveas"
                  style="push">
               <parameter
                     name="xstampp.commands.params.doSaveAs"
                     value="TRUE">
               </parameter>
            </command>
            <command
                  commandId="org.eclipse.ui.file.import"
                  label="%command.name.import"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.export"
                  label="%command.name.export"
                  style="push">
            </command>
            <separator
                  name="Separator 2"
                  visible="true">
            </separator>
            <command
                  commandId="astpa.switchworkspace"
                  id="xstampp.ui.menu.command.switchWS"
                  label="%command.name.switchworkspace"
                  style="pulldown"
                  tooltip="%command.tooltip.switchworkspace">
            </command>
            <command
                  commandId="xstampp.command.openPath"
                  label="%command.name.openWorkspace"
                  style="push">
               <parameter
                     name="xstampp.commandParameter.openFolder"
                     value="instance">
               </parameter>
            </command>
            <command
                  commandId="astpa.exit"
                  label="%command.name.exitPlatform"
                  style="push"
                  tooltip="%command.tooltip.exitPlatform">
            </command>
            <command
                  commandId="org.eclipse.pde.ui.externalizeStrings"
                  style="push">
            </command>
         </menu>
         <menu
               id="editMenu"
               label="%menu.name.edit">
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  helpContextId="Opens the preference page"
                  label="%command.name.preferences"
                  style="push">
            </command>
         </menu>
         <menu
               label="%menu.name.window">
            <command
                  commandId="org.eclipse.ui.window.switchToEditor"
                  label="%command.name.switchToEditor"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.views.showView"
                  label="%command.name.showView"
                  style="pulldown">
            </command>
            <separator
                  name="xstampp.separator1">
            </separator>
            <menu
                  label="%menu.name.navigation">
               <command
                     commandId="org.eclipse.ui.window.nextView"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.nextEditor"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.switchToEditor"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.previousView"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.previousEditor"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.ui.window.resetPerspective"
                  label="%command.name.resetPerspective"
                  style="push">
            </command>
         </menu>
         <menu
               id="helpMenu"
               label="%menu.name.help">
            <command
                  commandId="Welcome"
                  label="command.name.welcome"
                  style="push"
                  tooltip="%command.tooltip.showWelcome">
            </command>
            <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  icon="icons/xstampp/logox16.png"
                  id="x.ex.about"
                  label="%command.name.about"
                  style="push"
                  tooltip="%command.tooltip.showAbout">
            </command>
            <command
                  commandId="astpa.update"
                  label="%command.name.searchForUpdate"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.help.helpContents"
                  label="%command.name.helpContents"
                  style="push">
            </command>
            <command
                  commandId="xstampp.command.openPath"
                  label="%command.name.logPath"
                  style="push">
               <parameter
                     name="xstampp.commandParameter.openFolder"
                     value="log">
               </parameter>
            </command>
            <separator
                  name="xstampp.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.equinox.p2.ui.sdk.install"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.equinox.p2.ui.sdk.update"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.equinox.p2.ui.sdk.installationDetails"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?before=additions">
         <toolbar
               id="xstampp.main.toolbar">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  style="push">
            </command>
            <command
                  commandId="astpa.save"
                  style="push">
            </command>
            <command
                  commandId="xstampp.saveall"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.export"
                  style="push">
            </command>
         </toolbar>
         <separator
               name="xstampp.toolbar"
               visible="false">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:astpa.explorer">
         <command
               commandId="astpa.command.openStep"
               id="xstampp.explorer.menu.open"
               label="%command.name.open"
               style="toggle">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <instanceof
                        value="xstampp.ui.navigation.StepSelector">
                  </instanceof>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="astpa.commands.rename"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <instanceof
                        value="xstampp.ui.navigation.ProjectSelector">
                  </instanceof>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="astpa.save"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <instanceof
                        value="xstampp.ui.navigation.IProjectSelection">
                  </instanceof>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="astpa.commands.delete"
               icon="icons/eclipse/delete_obj.gif"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <instanceof
                        value="xstampp.ui.navigation.ProjectSelector">
                  </instanceof>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.file.export"
               style="push">
         </command>
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/collapseall2.gif"
               label="%command.name.collapseall"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="False">
            </parameter>
         </command>
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/expandall.gif"
               label="%command.name.expandall"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="True">
            </parameter>
         </command>
         <command
               commandId="xstampp.command.openPath"
               label="%command.name.openOutput"
               style="push">
            <parameter
                  name="xstampp.commandParameter.openFolder"
                  value="output">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:astpa.explorer">
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/collapseall2.gif"
               label="%command.name.collapseall"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="FALSE">
            </parameter>
         </command>
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/expandall.gif"
               label="%command.name.expandall"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="TRUE">
            </parameter>
         </command>
         <command
               commandId="org.eclipse.ui.window.preferences"
               style="push">
            <parameter
                  name="preferencePageId"
                  value="astpa.preferencePageColors">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?before=additions">
         <toolbar
               id="xstampp.toolbar.contribution.text">
            <separator
                  name="xstampp.separator1">
            </separator>
            <control
                  class="xstampp.ui.workbench.contributions.TextToolbarContribution">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeEditor">
                     <instanceof
                           value="xstampp.ui.editors.interfaces.ITextEditor">
                     </instanceof>
                  </with>
               </visibleWhen>
            </control>
         </toolbar>
         <toolbar
               id="xstampp.toolbar.contribution.zoombar">
            <control
                  class="xstampp.ui.workbench.contributions.SeperatorContribution">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeEditor">
                     <instanceof
                           value="xstampp.ui.workbench.contributions.IZoomContributor">
                     </instanceof>
                  </with>
               </visibleWhen>
            </control>
            <control
                  class="xstampp.ui.workbench.contributions.EditorContribution">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeEditor">
                     <instanceof
                           value="xstampp.ui.workbench.contributions.IZoomContributor">
                     </instanceof>
                  </with>
               </visibleWhen>
            </control>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="xstampp.run.toolbar">
            <command
                  commandId="xstampp.command.run"
                  icon="icons/buttons/export/run.png"
                  id="xstampp.toolbar.run.menu"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:xstampp.ui.menu.command.switchWS">
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <sourceProvider
            provider="xstampp.ui.menu.file.commands.CommandState">
         <variable
               name="xstampp.ui.menu.file.commands.savestate"
               priorityLevel="workbench">
         </variable>
         <variable
               name="xstampp.ui.menu.file.commands.textstate"
               priorityLevel="workbench">
         </variable>
         <variable
               name="xstampp.ui.menu.file.commands.saveAllState"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="xstampp.ui.menu.file.commands.Save"
            commandId="astpa.save">
         <enabledWhen>
            <with
                  variable="xstampp.ui.menu.file.commands.savestate">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="xstampp.ui.menu.file.commands.SaveAs"
            commandId="astpa.saveas">
         <enabledWhen>
            <with
                  variable="xstampp.ui.menu.file.commands.savestate">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="xstampp.ui.menu.file.commands.SaveAll"
            commandId="xstampp.saveall">
         <enabledWhen>
            <with
                  variable="xstampp.ui.menu.file.commands.saveAllState">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="xstampp.util.RunExportCommand"
            commandId="xstampp.command.run">
         <enabledWhen>
            <with
                  variable="selection">
               <instanceof
                     value="xstampp.ui.navigation.IProjectSelection">
               </instanceof>
            </with>
         </enabledWhen>
      </handler>ler>
   </extension>
